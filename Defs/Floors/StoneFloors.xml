<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <StuffedFloors.FloorTypeDef>
    	<defName>StuffedStoneFloor1</defName>
    	<label>checkered stone</label>
    	<description>An intricate interwoven design.</description>
        <texturePath>Floors/CheckeredStone</texturePath>
    	<stuffCategories>
            <li>Stony</li>
        </stuffCategories>
        <layerable>true</layerable>
        <affordances>
            <li>Light</li>
            <li>Heavy</li>
            <li>SmoothHard</li>
        </affordances>
        <statBases>
            <Beauty>3</Beauty>
            <WorkToBuild>135</WorkToBuild>
        </statBases>
        <stuffCost>5</stuffCost>
        <Fertility>0</Fertility>
        <constructEffect>ConstructDirt</constructEffect>
        <AcceptTerrainSourceFilth>True</AcceptTerrainSourceFilth>
        <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    </StuffedFloors.FloorTypeDef>
	
